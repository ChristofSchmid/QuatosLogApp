                <ComboBox x:Name="Combo_MaxRpm" HorizontalAlignment="Left" Margin="133,97,0,0" VerticalAlignment="Top" Width="56"/>
                <ComboBox x:Name="Combo_MinPwm" HorizontalAlignment="Left" Margin="133,124,0,0" VerticalAlignment="Top" Width="56"/>
                <ComboBox x:Name="Combo_MaxPwm" HorizontalAlignment="Left" Margin="133,150,0,0" VerticalAlignment="Top" Width="56"/>
                <Label x:Name="Lbl_SetPropBlades" Content="Nr. Prop Blades" HorizontalAlignment="Left" Margin="6,71,0,0" VerticalAlignment="Top" Height="26" Width="101"/>
