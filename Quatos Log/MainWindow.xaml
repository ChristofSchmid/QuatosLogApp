                <Label x:Name="Lbl_SetRpmStep" Content="RPM Step" HorizontalAlignment="Left" Margin="6,176,0,0" VerticalAlignment="Top" Width="101"/>
                <ComboBox x:Name="Combo_SetRpmStep" HorizontalAlignment="Left" Margin="133,178,0,0" VerticalAlignment="Top" Width="56"/>
