            <Grid HorizontalAlignment="Left" Height="101" Margin="0,0,0,-1.8" VerticalAlignment="Top" Width="270">
                <TextBox x:Name="RpmVal" HorizontalAlignment="Left" Height="23" Margin="190,8,0,0" TextWrapping="Wrap" Text="1000" VerticalAlignment="Top" Width="38" DataContext="{Binding Value, ElementName=RPMSlider, Mode=TwoWay}"/>
