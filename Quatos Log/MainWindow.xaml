                <Label x:Name="Th1" Content="Th" HorizontalAlignment="Left" Margin="0,99,0,0" VerticalAlignment="Top" Width="31" Height="22" FontSize="10" ScrollViewer.VerticalScrollBarVisibility="Disabled" Background="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" Grid.Row="1"/>
                <Label x:Name="Thrust" Content="Thrust" HorizontalAlignment="Left" Margin="36,100,0,0"  VerticalAlignment="Top" Width="57" Height="22" FontSize="10" ScrollViewer.VerticalScrollBarVisibility="Disabled" RenderTransformOrigin="0.5,0.5" Grid.Row="1" HorizontalContentAlignment="Right">
                    <Label.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="0.461"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Label.RenderTransform>
                </Label>
                <Label x:Name="UMin" Content="Rpm" HorizontalAlignment="Left" Margin="0,126,0,0" VerticalAlignment="Top" Width="31" Height="22" FontSize="10" ScrollViewer.VerticalScrollBarVisibility="Disabled" Background="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" Grid.Row="1"/>
