                <Button x:Name="SendSerialBtn" Content="Send" HorizontalAlignment="Left" Margin="204,11,0,0" VerticalAlignment="Top" Width="56" Height="22"/>
                <ComboBox x:Name="SerialCmdCombo" HorizontalAlignment="Left" Margin="6,39,0,0" VerticalAlignment="Top" Width="183" Height="22"/>
                <ComboBox x:Name="CmdValueCombo" HorizontalAlignment="Left" Margin="204,38,0,0" VerticalAlignment="Top" Width="56" SelectionChanged="ComboBox_SelectionChanged" Height="23"/>
