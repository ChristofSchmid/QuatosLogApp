        <Ellipse x:Name="HeartBeat" HorizontalAlignment="Left" Margin="443,32.2,0,608.2" Stroke="#FF0AF552" Width="18" RenderTransformOrigin="4.825,1.017" Fill="#FF3AF50A" Grid.Row="1"/>

    </Grid>


</Window>

