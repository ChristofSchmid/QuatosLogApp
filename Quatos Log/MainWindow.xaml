                <Label

                	Width="34" VerticalAlignment="Top" ScrollViewer.VerticalScrollBarVisibility="Disabled" RenderTransformOrigin="-0.133,0.667" x:Name="PWLow" Margin="0,135,0,0" Height="22" HorizontalAlignment="Left" FontSize="10" Content="Pwm" Background="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}"/>
