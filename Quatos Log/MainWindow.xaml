                <Button x:Name="ESCRun" Content="Start Motor" HorizontalAlignment="Center" Margin="0,71,0,0" VerticalAlignment="Top" Width="260" Height="23"/>
                <Label x:Name="MotRunLabel" Content="RPM" HorizontalAlignment="Left" Margin="228,6,0,0" VerticalAlignment="Top" Width="42" Height="25"/>
            </Grid>
