            <Grid HorizontalAlignment="Center" Height="71" Margin="0,0,0,-2.2" VerticalAlignment="Top" Width="270">
                <Button x:Name="BtnConnect" Content="Connect " HorizontalAlignment="Left" Margin="6,9,0,0" VerticalAlignment="Top" Width="130" Height="23"/>
                <ListBox x:Name="ComPort" HorizontalAlignment="Left" Height="22" Margin="147,10,0,0" VerticalAlignment="Top" Width="113"/>
                <CheckBox x:Name="ESC32Checkbox" Content="ESC32" HorizontalAlignment="Left" Margin="6,45,0,0" VerticalAlignment="Top" Height="16" Width="63" IsChecked="True" />
