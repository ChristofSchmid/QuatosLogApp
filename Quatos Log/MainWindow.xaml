                <Label

                	Width="57" VerticalAlignment="Top" ScrollViewer.VerticalScrollBarVisibility="Disabled" RenderTransformOrigin="-0.088,0.594" x:Name="MaxRpm" Margin="39,81,0,0" HorizontalAlignment="Left" FontSize="10" Content="Max Rpm" HorizontalContentAlignment="Right" Height="22"/>
                <Label

                	Width="57" VerticalAlignment="Top" ScrollViewer.VerticalScrollBarVisibility="Disabled" x:Name="MotorPoles" Margin="39,108,0,0" Height="22" HorizontalAlignment="Left" FontSize="10" Content="Blades" HorizontalContentAlignment="Right"/>
                <Label

                	Width="57" VerticalAlignment="Top" ScrollViewer.VerticalScrollBarVisibility="Disabled" RenderTransformOrigin="-0.088,0.594" x:Name="MinPWM" Margin="39,135,0,0" Height="22" HorizontalAlignment="Left" FontSize="10" Content="MinPWM" HorizontalContentAlignment="Right"/>
                <Label

                	Width="57" VerticalAlignment="Top" ScrollViewer.VerticalScrollBarVisibility="Disabled" RenderTransformOrigin="-0.088,0.594" x:Name="MaxPWM" Margin="39,162,0,0" Height="22" HorizontalAlignment="Left" FontSize="10" Content="MaxPWM" HorizontalContentAlignment="Right"/>
                <Label

