                <Button x:Name="StopButton" Content="Stop" Margin="0,154,0,0" VerticalAlignment="Top" BorderThickness="1" Height="23" Width="130" IsDefault="True" RenderTransformOrigin="0.508,0.237" HorizontalContentAlignment="Center" ClickMode="Press" HorizontalAlignment="Center"/>
                <Button x:Name="Rpm2Thrust" Content="Rpm 2 Thrust" Margin="6,35,0,0" VerticalAlignment="Top" Height="23" HorizontalAlignment="Left" Width="130" />
                <Button x:Name="MaxRpmBtn" Content="Run Max Rpm Test" HorizontalAlignment="Left" VerticalAlignment="Top" Width="130" Margin="6,7,0,0" Height="23"/>
                <Button x:Name="OLRpmThrust" Content="OL Rpm 100Hz" HorizontalAlignment="Left" Margin="6,63,0,0" VerticalAlignment="Top" Width="130" Height="23" RenderTransformOrigin="0.534,1.409"/>
                <Button x:Name="SinRpm" Content="OL Sin Rpm 100Hz" HorizontalAlignment="Left" Margin="6,91,0,0" VerticalAlignment="Top" Width="130" Height="23"/>
                <Button x:Name="Logdata" Content="Start Logdata" HorizontalAlignment="Left" Margin="154,91,0,0" VerticalAlignment="Top" Width="130" Height="23"/>
                <Button x:Name="CLRpmThrust1" Content="CL Rpm 1000Hz" HorizontalAlignment="Left" Margin="154,35,0,0" VerticalAlignment="Top" Width="130" Height="23"/>
                <Button x:Name="CLThrustStep1" Content="CL Thrust 100Hz" HorizontalAlignment="Left" Margin="154,63,0,0" VerticalAlignment="Top" Width="130" Height="23"/>
