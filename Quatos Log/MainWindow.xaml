                    <Label.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="0.461"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Label.RenderTransform>
                </Label>
            </Grid>
        </GroupBox>
        <GroupBox x:Name="GroupBox_3" Header="Tested Unit" HorizontalAlignment="Left" Margin="10,4.2,0,0" VerticalAlignment="Top" Height="91" Width="271" Grid.Row="1">
            <Grid HorizontalAlignment="Left" Height="70" VerticalAlignment="Top" Width="254" Margin="0,0,0,-1.2">
                <TextBox x:Name="TestUnitText" Height="23" Margin="0,10,0,0" TextWrapping="Wrap" Text="Motor, Prop, Voltage" VerticalAlignment="Top" HorizontalAlignment="Left" Width="246"/>
                <Button x:Name="Btn_SetTestedUnit" Content="Set" HorizontalAlignment="Left" Margin="0,42,0,0" VerticalAlignment="Top" Width="75"/>
            </Grid>
        </GroupBox>
        <GroupBox Header="Controller Output" HorizontalAlignment="Left" Margin="10,338.2,0,0" VerticalAlignment="Top" Height="311" Width="934" Grid.Row="1">
            <Grid HorizontalAlignment="Left" Height="296" Margin="0,10,-2.4,-54.8" VerticalAlignment="Top" Width="924">
                <TextBox x:Name="QuatSerText" HorizontalAlignment="Left" Margin="5,10,0,0" Text="TextBox" VerticalAlignment="Top" IsUndoEnabled="False" TextOptions.TextFormattingMode="Display" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" AllowDrop="False" Focusable="False" IsReadOnly="True" Height="233" Width="899" />
